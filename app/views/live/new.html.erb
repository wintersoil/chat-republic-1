<% if logged_in? %>
  <div class="cards-live">
    <% @users.each do |usr| %>
      <div class="card-live">
        <% if usr.id != current_user.id %>
          <% if usr.profile_picture.nil? == false %>
            <%= image_tag(usr.profile_picture.to_s, width: '100', height:'100', class:"profile-rounded") %>
          <% else %>
            <%= image_tag("https://d2x5ku95bkycr3.cloudfront.net/App_Themes/Common/images/profile/0_200.png", width:'100', height: '100', class:"profile-rounded") %>
          <% end %>
          <%= usr.first_name %> <%= usr.last_name %>
        <% end %>
      </div>
    <% end %>
  </div>
<% end %>
