<div>
  <div class="message-outer">
    <div class="message-wrap">
      <div class="message-boxing">
          <strong><%= msg.user.first_name %> <%= msg.user.last_name %> : </strong> <%= msg.body %><br>
          <span style="font-size:16px;color:lightgrey;"><%= time_ago_in_words(msg.created_at) %> ago</span>
      </div>
    </div>
  </div>
</div>
